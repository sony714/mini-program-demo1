/**index.wxss**/
.global{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.circle{
   width: 300rpx;
   height: 200rpx;
   border: 1px solid red;
   text-align: center;
   line-height: 200rpx;
   border-radius: 150rpx/100rpx;
   margin-top:25rpx;
   margin-bottom: 25rpx;
}

.numbers {
  display: flex;
  flex-direction: row;
}
.numbers>.number{
  padding-right:20rpx;
}